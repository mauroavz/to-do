 <v-form ref=\"form\" lazy-validation>\n        <v-text-field\n          v-model=\"note.title\"\n          :rules=\"[(v) => !!v || 'Titulo es requerido']\"\n          label=\"Titulo\"\n          required\n        ></v-text-field>\n\n        <v-text-field\n          v-model=\"note.description\"\n          :rules=\"[(v) => !!v || 'La descripcion es requerida']\"\n          label=\"Descripcion\"\n          required\n        ></v-text-field>\n      </v-form>\n\n      <v-btn color=\"primary\" class=\"mt-3\" @click=\"saveNote\">Subir</v-btn>\n    </div>\n\n    <div v-else>\n      <v-card class=\"mx-auto\">\n        <v-card-title>\n          ¡Enviado satisfactoriamente!\n        </v-card-title>\n\n        <v-card-subtitle>\n          Haga clic en el botón para agregar una nueva tarea\n        </v-card-subtitle>\n\n        <v-card-actions>\n          <v-btn color=\"success\" @click=\"newNote\">Agregar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NoteDataService from \"../services/NoteDataService\";\n\nexport default {\n  name: \"add-note\",\n  data() {\n    return {\n      note: {\n        id: null,\n        title: \"\",\n        description: \"\",\n        published: false,\n      },\n      submitted: false,\n    };\n  },\n  methods: {\n    saveNote() {\n      var data = {\n        title: this.note.title,\n        description: this.note.description,\n      };\n\n      NoteDataService.create(data)\n        .then((response) => {\n          this.note.id = response.data.id;\n          console.log(response.data);\n          this.submitted = true;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    newNote() {\n      this.submitted = false;\n      this.note = {};\n    },\n  },\n};\n</script>\n\n<style>\n.submit-form {\n  max-width: 350px;\n  padding: 60px;\n  background-color: #E7E7E7;\n  margin: 0 auto;\n}\n</style>"],"sourceRoot":"src/components"}]}